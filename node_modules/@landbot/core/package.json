    "lodash.set": "^4.3.2",
    "lodash.unset": "^4.5.2",
    "prettier": "^2.4.1",
    "rxjs": "^6.4.0",
    "ts-loader": "^9.2.5",
    "tslib": "^2.3.1",
    "typescript": "^4.3.5",
    "webpack": "^5.51.1",
    "webpack-cli": "^4.8.0"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS"
    }
  },
  "lint-staged": {
    "*.js": "eslint --fix"
