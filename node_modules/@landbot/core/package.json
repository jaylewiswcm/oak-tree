{
  "name": "@landbot/core",
  "version": "1.6.2",
  "description": "Landbot client core",
  "types": "./dist/index.d.ts",
  "main": "./dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://git.yexir.com/frontends/landbot-core.git"
  },
  "keywords": [
    "landbot",
    "core"
  ],
  "files": [
    "dist"
  ],
  "author": "landbot",
  "contributors": [
    {
      "name": "Eduardo Ruiz Gonz√°lez",
      "email": "eduardo.ruiz@landbot.io"
    }
  ],
  "license": "MIT",
  "scripts": {
    "build": "npm run clean && npm run build:prod",
    "build:prod": "webpack --mode production",
    "clean": "npx rimraf ./dist",
    "start": "webpack --progress --watch --mode development",
    "test": "jest --watch",
    "test:cov": "jest --coverage --ci",
    "deploy": "landbot-deploy-statics -c --source=./dist/landbot-core* --dist=landbot-core -r ./dist -S static.helloumi.com --credentials=$DEPLOYER_PROD"
  },
  "devDependencies": {
    "@babel/core": "^7.15.0",
    "@babel/eslint-parser": "^7.15.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.14.7",
    "@babel/plugin-transform-runtime": "^7.15.8",
    "@babel/preset-env": "^7.15.0",
    "@babel/preset-typescript": "^7.15.0",
    "@commitlint/cli": "^12.0.1",
    "@commitlint/config-conventional": "^12.0.1",
    "@frontends/deploy-statics": "^1.1.0",
    "@semantic-release/changelog": "^5.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/gitlab": "^6.0.9",
    "@types/jest": "^27.0.1",
    "@typescript-eslint/eslint-plugin": "^4.29.3",
    "babel-jest": "^27.2.0",
    "babel-loader": "^8.2.2",
    "browser-resolve": "^2.0.0",
    "eslint": "^7.32.0",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-jest": "^24.4.2",
    "eslint-plugin-prettier": "^4.0.0",
    "husky": "^4.3.8",
    "jest": "^27.2.0",
